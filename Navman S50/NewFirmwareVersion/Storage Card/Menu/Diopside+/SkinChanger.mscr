
	RegWriteDWord("HKLM", "SOFTWARE\Menu", "Exit",0)
	kill("WolfNCU.exe")

	spath   = SystemPath("ScriptPath")
	spath   = SubStr(spath, 1, ReverseFind(spath, "\")) 
	fConfig = spath\"WolfNCU.ini"
	
	if (IniRead(fConfig,"Skin","SkinName") eq "Diopside+V1")

		Iniwrite(fConfig,"Skin","SkinName","Diopside+V2")

	else

		Iniwrite(fConfig,"Skin","SkinName","Diopside+V1")

	endif

	run(spath\"menus.exe")
	