<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<resources>
<!-- page 01 -->
<resource class="view" title="Tutorial: Welcome" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="subtitlelarge" x="15" y="10" width="450" height="120">Welcome to the Navman tutorial</displayable>
	<displayable class="label" textstyle="standard" x="15" y="40" width="450" height="120">This tutorial will introduce you to the basic operation of your Navman, including how to navigate to your destination.</displayable>
	<displayable class="checkbox" rel="startup show tutorial" x="15" y="140" width="450" height="75" value="Show tutorial on start-up"/>
	<displayable class="label" textstyle="caution" x="310" y="188" width="450" height="120">Tap to continue</displayable>
	<displayable class="image" src="tutorial_arrow" x="365" y="210" />
</displayables>
</resource>
<!-- page 02 -->
<resource class="view" title="GPS" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" src="tutorial_gps_status" x="404" y="20" />
	<displayable class="label" textstyle="standard" x="15" y="10" width="390" height="120">Your Navman receives Global Positioning System (GPS) signals via a built-in antenna.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="79" width="450" height="120">To receive a strong signal, ensure that your Navman has an unobstructed view of the sky.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="125" width="450" height="120">To display the GPS Status screen, tap the compass, then tap and hold </displayable>
	<displayable class="image" src="tutorial_gps" x="115" y="150" />
	<displayable class="label" textstyle="standard" x="148" y="148" width="20" height="20">.</displayable>
</displayables>
</resource>
<!-- page 03 -->
<resource class="view" title="Main Menu" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" src="tutorial_main_menu" x="347" y="10" />
	<displayable class="label" textstyle="standard" x="15" y="10" width="330" height="100">The Main Menu allows you to enter an address for your trip, search for Points of Interest and customize preferences.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="102" width="450" height="100">You can access the Main Menu by tapping the status bar at the bottom of any screen.</displayable>
</displayables>
</resource>
<!-- page 04 -->
<resource class="view" title="Navigation" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" x="347" y="35" src="tutorial_destination1"/>
	<displayable class="image" x="347" y="115" src="tutorial_destination2"/>
	<displayable class="label" textstyle="standardbold" x="15" y="8" width="430" height="50">How do I plan my first trip?</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="35" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="35" width="315" height="80">From the Main Menu, tap</displayable>
	<displayable class="image" x="225" y="37" src="tutorial_address_button"/>
	<displayable class="label" textstyle="standard" x="256" y="35" width="295" height="120"> (Address).</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="83" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="85" width="315" height="80">Enter the country, city, street and house number of your destination.</displayable>
</displayables>
</resource>
<!-- page 05 -->
<resource class="view" title="Navigation" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" x="347" y="10" src="tutorial_destination3"/>
	<displayable class="image" x="347" y="90" src="tutorial_map_screen1"/>
	<displayable class="label" textstyle="standardbold" x="15" y="8" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="10" width="295" height="80">When you have entered your address, tap</displayable>
	<displayable class="image" x="65" y="37" src="tutorial_go_button"/>
	<displayable class="label" textstyle="standard" x="92" y="33" width="315" height="80"> (Go).</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="83" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="85" width="295" height="80">Your Navman will calculate your route and  display the 3D Map screen.</displayable>
</displayables>
</resource>
<!-- page 06 -->
<resource class="view" title="Map Screens" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="standard" x="15" y="10" width="450" height="60">There are five map screens used to view the route to your destination:</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="55" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="55" width="415" height="24">3D Map</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="75" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="75" width="415" height="24">2D Map</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="95" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="95" width="415" height="24">Traffic Overview (if available)</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="115" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="115" width="415" height="24">Route Overview </displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="135" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="135" width="310" height="24">Turn-by-Turn</displayable>
	<displayable class="image" x="347" y="50" src="tutorial_map_screen2"/>
	<displayable class="label" textstyle="standard" x="15" y="160" width="335" height="60">To cycle through the map screens, tap </displayable>
	<displayable class="image" x="310" y="162" src="tutorial_map_button"/>
	<displayable class="label" textstyle="standard" x="334" y="160" width="120" height="20">.</displayable>
</displayables>
</resource>
<!-- page 07 -->
<resource class="view" title="Volume Control" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="standard" x="15" y="10" width="450" height="120">To mute the volume, tap the compass, then tap </displayable>
	<displayable class="image" src="tutorial_volume1" x="385" y="13" />
	<displayable class="label" textstyle="standard" x="415" y="10" width="20" height="20">.</displayable>
	<displayable class="image" src="tutorial_prefs_volume" x="347" y="55" />
	<displayable class="label" textstyle="standard" x="15" y="55" width="335" height="120">To adjust the volume, tap and hold</displayable>
	<displayable class="image" src="tutorial_volume2" x="290" y="58" />
	<displayable class="label" textstyle="standard" x="15" y="78" width="335" height="120">to display the Volume preference screen.</displayable>
</displayables>
</resource>
<!-- page 08 -->
<resource class="view" title="Installation" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" src="tutorial_install" x="300" y="90" />
	<displayable class="label" textstyle="standard" x="15" y="10" width="450" height="120">Position your Navman according to the installation instructions in the Quick Start Guide.</displayable>
</displayables>
</resource>
