<resources>
<!-- page 01 -->
<resource class="view" title="Lernprogramm: Willkommen" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="subtitlelarge" x="15" y="10" width="450" height="120">Willkommen beim Navman Lernprogramm</displayable>
	<displayable class="label" textstyle="standard" x="15" y="40" width="450" height="120">Dieses Lernprogramm wird Sie in die Grundfunktionen Ihres Navman einführen.</displayable>
	<displayable class="checkbox" rel="startup show tutorial" x="15" y="140" width="450" height="75" value="Lernprogramm beim Start anzeigen"/>
	<displayable class="label" textstyle="caution" x="250" y="188" width="450" height="120">Tippen Sie, um fortzufahren</displayable>
	<displayable class="image" src="tutorial_arrow" x="365" y="210" />
</displayables>
</resource>
<!-- page 02 -->
<resource class="view" title="GPS" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" src="tutorial_gps_status" x="404" y="20" />
	<displayable class="label" textstyle="standard" x="15" y="10" width="390" height="120">Ihr Navman empfängt über eine eingebaute Antenne Signale des Global Positioning Systems (GPS).</displayable>
	<displayable class="label" textstyle="standard" x="15" y="79" width="450" height="120">Um ein starkes Signal zu erhalten, vergewissern Sie sich, dass Ihr Navman eine freie Sichtverbindung zum Himmel hat.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="148" width="450" height="120">Um den Bildschirm GPS-Status anzuzeigen, tippen Sie auf den Kompass und drücken und halten Sie dann </displayable>
	<displayable class="image" src="tutorial_gps" x="370" y="173" />
	<displayable class="label" textstyle="standard" x="403" y="171" width="20" height="20">.</displayable>
</displayables>
</resource>
<!-- page 03 -->
<resource class="view" title="Hauptmenü" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" src="tutorial_main_menu" x="347" y="10" />
	<displayable class="label" textstyle="standard" x="15" y="10" width="330" height="100">Das Hauptmenüer möglicht es Ihnen, eine Adresse für Ihre Fahrt einzugeben, nach Sonderzielen zu suchen und die Einstellungen anzupassen.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="102" width="450" height="100">Sie können auf das Hauptmenü zugreifen, indem Sie auf die Statuszeile, unten auf jedem Bildschirm, klicken.</displayable>
</displayables>
</resource>
<!-- page 04 -->
<resource class="view" title="Navigation" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" x="347" y="35" src="tutorial_destination1"/>
	<displayable class="image" x="347" y="115" src="tutorial_destination2"/>
	<displayable class="label" textstyle="standardbold" x="15" y="8" width="430" height="50">Wie plane ich meine erste Fahrt?</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="35" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="35" width="315" height="80">Tippen Sie im Hauptmenü auf </displayable>
	<displayable class="image" x="260" y="37" src="tutorial_address_button"/>
	<displayable class="label" textstyle="standard" x="30" y="58" width="295" height="120"> (Adresse).</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="83" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="85" width="315" height="80">Geben Sie das Land, die Stadt, die Straße und die Hausnummer Ihres Ziels ein.</displayable>
</displayables>
</resource>
<!-- page 05 -->
<resource class="view" title="Navigation" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" x="347" y="10" src="tutorial_destination3"/>
	<displayable class="image" x="347" y="90" src="tutorial_map_screen1"/>
	<displayable class="label" textstyle="standardbold" x="15" y="8" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="10" width="295" height="80">Nachdem Sie Ihre Adresse eingegeben haben, tippen Sie auf </displayable>
	<displayable class="image" x="200" y="37" src="tutorial_go_button"/>
	<displayable class="label" textstyle="standard" x="227" y="33" width="315" height="80"> (Start).</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="83" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="85" width="295" height="80">Ihr Navman wird die Route berechnen und auf dem Bildschirm eine 3D-Karte anzeigen.</displayable>
</displayables>
</resource>
<!-- page 06 -->
<resource class="view" title="Karten-Bildschirme" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="standard" x="15" y="10" width="450" height="60">Zur Anzeige der Zielroute stehen fünf Kartenbildschirme zur Verfügung:</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="55" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="55" width="415" height="24">3D-Karte</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="75" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="75" width="415" height="24">2D-Karte</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="95" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="95" width="415" height="24">Verkehrsübersicht (falls verfügbar)</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="115" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="115" width="415" height="24">Routenübersicht</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="135" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="135" width="310" height="24">Turn-by-Turn</displayable>
	<displayable class="image" x="347" y="50" src="tutorial_map_screen2"/>
	<displayable class="label" textstyle="standard" x="15" y="160" width="450" height="60">Um durch die Kartenbildschirme zu blättern, tippen Sie auf </displayable>
	<displayable class="image" x="15" y="184" src="tutorial_map_button"/>
	<displayable class="label" textstyle="standard" x="39" y="183" width="120" height="20">.</displayable>
</displayables>
</resource>
<!-- page 07 -->
<resource class="view" title="Lautstärkeregler" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="standard" x="15" y="10" width="450" height="120">Um die Lautstärke stumm zu schalten, tippen Sie auf den Kompass und dann auf </displayable>
	<displayable class="image" src="tutorial_volume1" x="190" y="35" />
	<displayable class="label" textstyle="standard" x="220" y="33" width="20" height="20">.</displayable>
	<displayable class="image" src="tutorial_prefs_volume" x="347" y="55" />
	<displayable class="label" textstyle="standard" x="15" y="56" width="335" height="120">Zur Anpassung der Lautstärke tippen und halten Sie </displayable>
	<displayable class="image" src="tutorial_volume2" x="98" y="81" />
	<displayable class="label" textstyle="standard" x="15" y="102" width="335" height="120">um auf den Einstellungsbildschirm Lautstärke zu gelangen.  </displayable>
</displayables>
</resource>
