<!-- page 01 -->
<resource class="view" title="Tutorial: Benvenuti" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="subtitlelarge" x="15" y="10" width="450" height="120">Benvenuti al tutorial del Navman</displayable>
	<displayable class="label" textstyle="standard" x="15" y="40" width="450" height="120">Questo tutorial presenta il funzionamento di base del Navman, compreso il modo di dirigersi verso la destinazione.</displayable>
	<displayable class="checkbox" rel="startup show tutorial" x="15" y="140" width="450" height="75" value="Mostra tutorial all'inizio"/>
	<displayable class="label" textstyle="caution" x="250" y="188" width="450" height="120">Selezionare per continuare</displayable>
	<displayable class="image" src="tutorial_arrow" x="365" y="210" />
</displayables>
</resource>
<!-- page 02 -->
<resource class="view" title="GPS" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" src="tutorial_gps_status" x="404" y="20" />
	<displayable class="label" textstyle="standard" x="15" y="10" width="390" height="120">Il Navman riceve i segnali del Sistema di posizionamento globale (GPS) tramite l'antenna integrata.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="79" width="450" height="120">Per ricevere un segnale forte, verificare che il Navman disponga di una visuale chiara del cielo.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="125" width="450" height="120">Per visualizzare la schermata Stato GPS, selezionare la bussola, quindi mantenere premuto </displayable>
	<displayable class="image" src="tutorial_gps" x="290" y="150" />
	<displayable class="label" textstyle="standard" x="323" y="148" width="20" height="20">.</displayable>
</displayables>
</resource>
<!-- page 03 -->
<resource class="view" title="Menu principale" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" src="tutorial_main_menu" x="347" y="10" />
	<displayable class="label" textstyle="standard" x="15" y="10" width="330" height="100">Il menu principale consente di immettere un indirizzo per il viaggio, cercare Punti di interesse e personalizzare le preferenze.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="102" width="450" height="100">Si può accedere al menu principale selezionando la barra di stato nella parte inferiore delle schermate.</displayable>
</displayables>
</resource>
<!-- page 04 -->
<resource class="view" title="Navigazione" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" x="347" y="35" src="tutorial_destination1"/>
	<displayable class="image" x="347" y="115" src="tutorial_destination2"/>
	<displayable class="label" textstyle="standardbold" x="15" y="8" width="430" height="50">Pianificazione del primo viaggio</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="35" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="35" width="315" height="80">Nel menu principale, selezionare </displayable>
	<displayable class="image" x="30" y="60" src="tutorial_address_button"/>
	<displayable class="label" textstyle="standard" x="61" y="58" width="295" height="120"> (Indirizzo).</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="83" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="85" width="315" height="80">Immettere Paese, città, strada e numero civico di destinazione.</displayable>
</displayables>
</resource>
<!-- page 05 -->
<resource class="view" title="Navigazione" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" x="347" y="10" src="tutorial_destination3"/>
	<displayable class="image" x="347" y="90" src="tutorial_map_screen1"/>
	<displayable class="label" textstyle="standardbold" x="15" y="8" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="10" width="295" height="80">Dopo aver immesso l'indirizzo, selezionare </displayable>
	<displayable class="image" x="120" y="37" src="tutorial_go_button"/>
	<displayable class="label" textstyle="standard" x="147" y="33" width="315" height="80"> (Vai).</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="83" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="85" width="295" height="80">Il Navman calcolerà e visualizzerà il percorso nella schermata Mappa in 3D.</displayable>
</displayables>
</resource>
<!-- page 06 -->
<resource class="view" title="Schermate mappa" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="standard" x="15" y="10" width="450" height="60">Vi sono cinque schermate di mappa usate per visualizzare il percorso per arrivare a destinazione:</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="55" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="55" width="415" height="24">Mappa in 3D</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="75" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="75" width="415" height="24">Mappa in 2D</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="95" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="95" width="415" height="24">Panoramica sul traffico (se disponibile)</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="115" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="115" width="415" height="24">Panoramica sul percorso</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="135" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="135" width="310" height="24">Svolta dopo svolta</displayable>
	<displayable class="image" x="347" y="50" src="tutorial_map_screen2"/>
	<displayable class="label" textstyle="standard" x="15" y="160" width="450" height="60">Per passare da una schermata di mappa all'altra, selezionare </displayable>
	<displayable class="image" x="105" y="183" src="tutorial_map_button"/>
	<displayable class="label" textstyle="standard" x="129" y="183" width="120" height="20">.</displayable>
</displayables>
</resource>
