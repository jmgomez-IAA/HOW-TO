<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<resources>
<!-- page 01 -->
<resource class="view" title="Lesprogramma: Welkom" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="subtitlelarge" x="15" y="10" width="450" height="120">Welkom bij het lesprogramma van Navman</displayable>
	<displayable class="label" textstyle="standard" x="15" y="40" width="450" height="120">Dit lesprogramma is een inleiding tot de basisbediening van uw Navman, en leert u tevens hoe u naar uw bestemming kunt navigeren.</displayable>
	<displayable class="checkbox" rel="startup show tutorial" x="15" y="140" width="450" height="75" value="Toon lesprogramma bij opstarten"/>
	<displayable class="label" textstyle="caution" x="280" y="188" width="450" height="120">Tik om verder te gaan</displayable>
	<displayable class="image" src="tutorial_arrow" x="365" y="210" />
</displayables>
</resource>
<!-- page 02 -->
<resource class="view" title="GPS" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" src="tutorial_gps_status" x="404" y="20" />
	<displayable class="label" textstyle="standard" x="15" y="10" width="390" height="120">Uw Navman ontvangt Global Positioning System (GPS) signalen via een ingebouwde antenne.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="79" width="450" height="120">Om een sterk signaal te ontvangen dient u ervoor te zorgen dat uw Navman een onbelemmerd zicht heeft op de lucht.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="148" width="450" height="120">Voor de weergave van het GPS Status-scherm, tik op het kompas en dan tik en houd ingedrukt op </displayable>
	<displayable class="image" src="tutorial_gps" x="325" y="173" />
	<displayable class="label" textstyle="standard" x="358" y="171" width="300" height="20">.</displayable>
</displayables>
</resource>
<!-- page 03 -->
<resource class="view" title="Hoofdmenu" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" src="tutorial_main_menu" x="347" y="10" />
	<displayable class="label" textstyle="standard" x="15" y="10" width="330" height="100">Met het Hoofdmenukunt u een adres invoeren voor uw reis, zoeken naar Nuttige adressen en voorkeuren aanpassen.</displayable>
	<displayable class="label" textstyle="standard" x="15" y="102" width="450" height="100">U komt in het Hoofdmenu door op de statusbalk te tikken onderaan ieder scherm.</displayable>
</displayables>
</resource>
<!-- page 04 -->
<resource class="view" title="Navigatie" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" x="347" y="35" src="tutorial_destination1"/>
	<displayable class="image" x="347" y="115" src="tutorial_destination2"/>
	<displayable class="label" textstyle="standardbold" x="15" y="8" width="430" height="50">Hoe plan ik mijn eerste reis?</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="35" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="35" width="315" height="80">Tik in het hoofdmenu op </displayable>
	<displayable class="image" x="225" y="37" src="tutorial_address_button"/>
	<displayable class="label" textstyle="standard" x="256" y="35" width="295" height="120"> (Adres).</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="83" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="85" width="315" height="80">Geef het land in, de stad, straat en huisnummer van uw bestemming.</displayable>
</displayables>
</resource>
<!-- page 05 -->
<resource class="view" title="Navigatie" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="image" x="347" y="10" src="tutorial_destination3"/>
	<displayable class="image" x="347" y="90" src="tutorial_map_screen1"/>
	<displayable class="label" textstyle="standardbold" x="15" y="8" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="10" width="295" height="80">Als u uw adres heeft ingegeven, tik dan op </displayable>
	<displayable class="image" x="85" y="37" src="tutorial_go_button"/>
	<displayable class="label" textstyle="standard" x="112" y="33" width="315" height="80">(Ga).</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="83" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="30" y="85" width="295" height="80">Uw Navman zal uw route berekenen en het 3D Navigatie-scherm weergeven.</displayable>
</displayables>
</resource>
<!-- page 06 -->
<resource class="view" title="navigatieschermen" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="standard" x="15" y="10" width="450" height="60">Er worden vijf navigatieschermen gebruikt om de route naar uw bestemming te bekijken:</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="55" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="55" width="415" height="24">3D Navigatie</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="75" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="75" width="415" height="24">2D Navigatie</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="95" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="95" width="415" height="24">Verkeersoverzicht (indien beschikbaar)</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="115" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="115" width="415" height="24">Route-overzicht</displayable>
	<displayable class="label" textstyle="standardbold" x="15" y="135" width="20" height="20">•</displayable>
	<displayable class="label" textstyle="standard" x="35" y="135" width="310" height="24">bij iedere afslag</displayable>
	<displayable class="image" x="347" y="50" src="tutorial_map_screen2"/>
	<displayable class="label" textstyle="standard" x="15" y="160" width="450" height="60">Druk om door de navigatieschermen te lopen op </displayable>
	<displayable class="image" x="385" y="162" src="tutorial_map_button"/>
	<displayable class="label" textstyle="standard" x="409" y="160" width="120" height="20">.</displayable>
</displayables>
</resource>
<!-- page 07 -->
<resource class="view" title="Volumebediening" backbutton="true" forwardbutton="true">
<displayables>
	<displayable class="label" textstyle="standard" x="15" y="10" width="450" height="120">Om het volume uit te zetten tikt u op het kompas en tik op </displayable>
	<displayable class="image" src="tutorial_volume1" x="40" y="35" />
	<displayable class="label" textstyle="standard" x="70" y="33" width="20" height="20">.</displayable>
	<displayable class="image" src="tutorial_prefs_volume" x="347" y="85" />
	<displayable class="label" textstyle="standard" x="15" y="56" width="335" height="120">Om het volume aan te passen tikt u op </displayable>
	<displayable class="image" src="tutorial_volume2" x="315" y="59" />
	<displayable class="label" textstyle="standard" x="15" y="79" width="335" height="120"> en houd deze ingedrukt om in het Volumevoorkeuren-scherm te komen.  </displayable>
</displayables>
</resource>
