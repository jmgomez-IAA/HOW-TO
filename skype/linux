	    if (typeof s_el.trackPageProperties != 'undefined') {
	    	_obj = $.extend(_obj, _t, s_el.trackPageProperties);
	    }
    }
    
    if (_obj != null) {
    	
    	s.trackPage(_obj);
    	
    } else {
    	
    	s.trackPage(_t);
    	
    }
    
    $(function() {
        $(document).on('click.sc', 'a[data-sc]:not(select)', function(e) {
            
            if (typeof s_el[$(this).attr('data-sc')] == 'undefined')
                return;
            
            var tracking_obj = s_el[$(this).attr('data-sc')].click;
            
            if (typeof tracking_obj === 'function'){
                tracking_obj = tracking_obj.call(); 
            }
            
            if (tracking_obj.asPage === true) {
                s.trackPage(tracking_obj);
            } else {
                s.trackAction(tracking_obj);
            }
        });
    });
